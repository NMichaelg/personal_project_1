{
"manifest_version": 3,
"name": "Focus Grinder",
"version": "1.0",
"description": "Block distracting websites and stay focused.",
"permissions": ["tabs", "storage"],
"background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Focus Blocker"
  }
}
